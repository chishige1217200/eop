#include <stdio.h>

int split(char *str, char *ret[], char sep, int max)
{
  int c = 0;

  printf("split start.\n");
  while(*str != '\0')
    {
      if(*str != sep)/**/
	{
	*ret[c] == *str;
	}


    }
  printf("split quit.\n");
  return c - 1;
}

int main(void)
{
  char str[1025] = 0;
  char ret[5][1025] = 0;
  char sep = ',';
  int max = 5;
  int i = 0;
  
  printf("Please input str.\n");
  scanf("%s", str);

  split(str, {&ret[0][0], &ret[1][0], &ret[2][0], &ret[3][0], &ret[4][0]}, &sep, max);

  for(i = 0;i < max; i++)
    {
      if(ret[i][0] != '\0')
	printf("%d:%s\n", i,str[i][0]);
    }
  return 0;
}
